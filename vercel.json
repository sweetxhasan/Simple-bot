{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/bot.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/setwebhook",
      "dest": "/api/bot.py",
      "methods": ["GET"]
    },
    {
      "src": "/getwebhook",
      "dest": "/api/bot.py",
      "methods": ["GET"]
    },
    {
      "src": "/health",
      "dest": "/api/bot.py",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "/api/bot.py",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/bot.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
